# FastAPI Proxy Environment Configuration

# ERPNext Backend
ERPNEXT_BASE_URL=http://localhost:8000
ERPNEXT_API_KEY=your_api_key_here
ERPNEXT_API_SECRET=your_api_secret_here

# FastAPI Server
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8001
FASTAPI_ENV=development
FASTAPI_DEBUG=false

# Security
JWT_SECRET_KEY=your_jwt_secret_key_here_minimum_32_characters_long
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=60

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_READ_PER_MINUTE=100
RATE_LIMIT_WRITE_PER_MINUTE=20
RATE_LIMIT_GLOBAL_PER_MINUTE=1000
RATE_LIMIT_BURST_PER_SECOND=50

# Caching
CACHE_ENABLED=true
CACHE_REDIS_URL=redis://localhost:6379/0
CACHE_DEFAULT_TTL=60

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
CORS_ALLOW_CREDENTIALS=true

# Logging
LOG_LEVEL=INFO
